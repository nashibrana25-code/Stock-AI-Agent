{
  "version": 2,
  "builds": [
    {
      "src": "services/api-gateway/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "services/api-gateway/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "services/api-gateway/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
